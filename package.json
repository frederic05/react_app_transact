    "nouislider": "15.2.0",
    "npm": "^8.7.0",
    "primereact": "^8.0.1",
    "react": "17.0.2",
    "react-bootstrap-modal": "^4.2.0",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "^17.0.2",
    "react-otp-input": "^2.4.0",
